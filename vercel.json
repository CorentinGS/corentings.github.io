{
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "Referrer-Policy",
					"value": "strict-origin-when-cross-origin"
				},
				{
					"key": "Permissions-Policy",
					"value": "geolocation=(), camera=(), microphone=()"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		}
	]
}
