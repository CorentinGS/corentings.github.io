{
	"name": "corentings.dev",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "tinacms dev -c \"astro dev --host\"",
		"start": "astro dev",
		"build": "tinacms build && astro build",
		"preview": "astro preview",
		"sync": "astro sync",
		"astro": "astro",
		"format": "prettier --plugin-search-dir=. --write .",
		"lint": "eslint .",
		"format:eslint": "eslint --ignore-path .gitignore --ext js,jsx,ts,tsx,mjs,cjs,astro --fix .",
		"format:imports": "organize-imports-cli ./tsconfig.json",
		"lint:types": "tsc -b",
		"lint:prettier": "prettier . \"**/*.astro\" --cache --write --list-different",
		"i18n": "npx astro-i18next generate",
		"serve": "node ./dist/server/entry.mjs"
	},
	"dependencies": {
		"@astrojs/mdx": "2.3.1",
		"@astrojs/node": "8.2.5",
		"@astrojs/prism": "^3.0.0",
		"@astrojs/tailwind": "5.1.0",
		"@astrojs/vercel": "7.5.4",
		"@fontsource/do-hyeon": "^5.0.17",
		"@fontsource/nunito": "^5.0.16",
		"@fontsource/syne": "^5.0.18",
		"@iconify/json": "^2.2.184",
		"@rollup/plugin-json": "^6.1.0",
		"@vercel/speed-insights": "^1.0.10",
		"astro": "4.7.1",
		"astro-font": "^0.0.80",
		"astro-i18next": "1.0.0-beta.21",
		"astro-iconify": "^1.2.0",
		"astro-seo-schema": "^4.0.0",
		"daisyui": "^4.7.2",
		"i18next": "22.4.11",
		"prettier-config-standard": "^7.0.0",
		"reading-time": "^1.5.0",
		"sharp": "^0.33.2",
		"tailwind-merge": "^2.2.1",
		"tailwindcss": "^3.4.1",
		"theme-change": "^2.5.0",
		"tinacms": "^1.5.28"
	},
	"devDependencies": {
		"@astrojs/rss": "4.0.5",
		"@astrojs/sitemap": "3.1.4",
		"@pagefind/default-ui": "^1.0.4",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/line-clamp": "0.4.4",
		"@tailwindcss/typography": "0.5.13",
		"@tinacms/cli": "^1.5.39",
		"@types/node": "^20.11.19",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"astro-compress": "^2.2.10",
		"astro-compressor": "^0.4.1",
		"astro-eslint-parser": "^1.0.0",
		"autoprefixer": "^10.4.17",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-astro": "^1.0.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-tailwindcss": "^3.14.2",
		"motion": "^10.16.4",
		"npm-run-all2": "^5.0.0",
		"organize-imports-cli": "^0.10.0",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"prettier-plugin-astro": "^0.13.0",
