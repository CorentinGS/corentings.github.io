{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": ["astro/client"],
    "baseUrl": ".",
    "importsNotUsedAsValues": "remove"
  },
  "include": ["**/*", ".eslintrc.cjs"]
}
