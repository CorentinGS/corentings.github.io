{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "render"
		},
		{
			"src": "^\\/de\\/blog(?:\\/(.*?))?\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/fr\\/blog(?:\\/(.*?))?\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/rss\\.xml\\/?$",
			"dest": "render"
		}
	]
}